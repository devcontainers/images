{
	"version": "1.0.2",
	"variants": [
		"8.2-apache-bookworm",
		"8.1-apache-bookworm",
		"8.2-apache-bullseye",
		"8.1-apache-bullseye",
		"8.0-apache-bullseye",
		"8.2-apache-buster",
		"8.1-apache-buster",
		"8.0-apache-buster"
	],
	"build": {
		"latest": "8.2-apache-bookworm",
		"rootDistro": "debian",
		"architectures": {
			"8.2-apache-bookworm": [
				"linux/amd64",
				"linux/arm64"
			],
			"8.1-apache-bookworm": [
				"linux/amd64",
				"linux/arm64"
			],
			"8.2-apache-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"8.1-apache-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"8.0-apache-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"8.2-apache-buster": [
				"linux/amd64"
			],
			"8.1-apache-buster": [
				"linux/amd64"
			],
			"8.0-apache-buster": [
				"linux/amd64"
			]
		},
		"tags": [
			"php:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"8.2-apache-bookworm": [
				"php:${VERSION}-8",
				"php:${VERSION}-8.2",
				"php:${VERSION}-8-bookworm",
				"php:${VERSION}-8.2-bookworm",
				"php:${VERSION}-bookworm"
			],
			"8.1-apache-bookworm": [
				"php:${VERSION}-8.1",
				"php:${VERSION}-8.1-bookworm"
			],
			"8.2-apache-bullseye": [
				"php:${VERSION}-8-bullseye",
				"php:${VERSION}-8.2-bullseye",
				"php:${VERSION}-bullseye"
			],
			"8.1-apache-bullseye": [
				"php:${VERSION}-8.1-bullseye"
			],
			"8.0-apache-bullseye": [
				"php:${VERSION}-8.0",
				"php:${VERSION}-8.0-bullseye"
			],
			"8.2-apache-buster": [
				"php:${VERSION}-8-buster",
				"php:${VERSION}-8.2-buster",
				"php:${VERSION}-buster"
			],
			"8.1-apache-buster": [
				"php:${VERSION}-8.1-buster"
			],
			"8.0-apache-buster": [
				"php:${VERSION}-8.0-buster"
			]
		}
	},
	"dependencies": {
		"image": "php:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/php",
		"apt": [
			{
				"cgIgnore": false,
				"name": "yarn"
			}
		],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm"
		},
		"other": {
			"git": {},
			"Xdebug": {
				"path": "/usr/local/lib/php/extensions"
			},
			"Composer": {
				"path": "/usr/local"
			}
		},
		"languages": {
			"PHP": {
				"cgIgnore": true,
				"path": "/usr/local"
			}
		}
	}
}