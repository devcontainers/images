{
	"version": "1.0.5",
	"variants": [
		"3.2-bookworm",
		"3.1-bookworm",
		"3.2-bullseye",
		"3.1-bullseye",
		"3.0-bullseye",
		"3.2-buster",
		"3.1-buster",
		"3.0-buster"
	],
	"build": {
		"latest": "3.2-bookworm",
		"rootDistro": "debian",
		"architectures": {
			"3.2-bookworm": [
				"linux/amd64",
				"linux/arm64"
			],
			"3.1-bookworm": [
				"linux/amd64",
				"linux/arm64"
			],
			"3.2-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"3.1-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"3.0-bullseye": [
				"linux/amd64",
				"linux/arm64"
			],
			"3.2-buster": [
				"linux/amd64"
			],
			"3.1-buster": [
				"linux/amd64"
			],
			"3.0-buster": [
				"linux/amd64"
			]
		},
		"tags": [
			"ruby:${VERSION}-${VARIANT}"
		],
		"variantTags": {
			"3.2-bookworm": [
				"ruby:${VERSION}-3",
				"ruby:${VERSION}-3.2",
				"ruby:${VERSION}-3-bookworm",
				"ruby:${VERSION}-bookworm"
			],
			"3.1-bookworm": [
				"ruby:${VERSION}-3.1"
			],
			"3.0-bullseye": [
				"ruby:${VERSION}-3.0"
			],
			"3.2-bullseye": [
				"ruby:${VERSION}-3-bullseye",
				"ruby:${VERSION}-bullseye"
			],
			"3.2-buster": [
				"ruby:${VERSION}-3-buster",
				"ruby:${VERSION}-buster"
			]
		}
	},
	"dependencies": {
		"image": "ruby:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/ruby",
		"debian": [
			{
				"cgIgnore": false,
				"name": "yarn"
			},
			"build-essential"
		],
		"git": {
			"Oh My Zsh!": "/home/vscode/.oh-my-zsh",
			"nvm": "/usr/local/share/nvm",
			"rbenv": "/usr/local/share/rbenv",
			"ruby-build": "/usr/local/share/ruby-build"
		},
		"gem": [
			"rake",
			"ruby-debug-ide",
			"debase"
		],
		"other": {
			"git": {},
			"rvm": null
		},
		"languages": {
			"Ruby": {
				"cgIgnore": true,
				"path": "/usr/local"
			}
		}
	}
}